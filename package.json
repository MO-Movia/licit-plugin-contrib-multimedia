{
  "name": "@modusoperandi/licit-multimedia",
  "version": "0.0.7",
  "subversion": "1",
  "description": "Multimedia plugin built with ProseMirror",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bundleDependencies": [],
  "scripts": {
    "clean": "rm -rf dist/ coverage/ bin/ && rm -f modusoperandi-licit-multimedia-*.*.*.tgz",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts,.tsx",
    "build:dist": "npm run clean && npm run build:ts && npm run webpack && npm run build:babel && npm run build:css",
    "webpack": "webpack",
    "build:css": "cp src/ui/*.css dist/ui",
    "lint:ts": "eslint src --ext .ts,.tsx --fix",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint": "npm run lint:css & npm run lint:ts",
    "ci:build": "npm run clean && npm run build:ts && npm run webpack && npm run build:babel && npm run build:css",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "prepare": "npm run build:dist",
    "test": "jest --coverage",
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i",
    "ci:bom": "cyclonedx-bom -o dist/bom.xml"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@babel/plugin-proposal-do-expressions": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-parameters": "^7.18.8",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/plugin-transform-typescript": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@cyclonedx/bom": "^3.0.3",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.2.0",
    "babel-loader": "8.2.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.10",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "jest-json": "^2.0.0",
    "jest-junit": "^14.0.1",
    "jest-prosemirror": "^2.0.6",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.14.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-prettier": "^2.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "url": "^0.11.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@modusoperandi/licit-ui-commands": "^0.1.16",
    "@types/node": "^16.10.1",
    "axios": "^1.1.3",
    "orderedmap": "^1.1.8",
    "prop-types": "^15.8.1",
    "rewire": "^6.0.0"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "!(*test|*.setup).ts": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
